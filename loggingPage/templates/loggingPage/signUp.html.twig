{% extends 'loggingPage/base.html.twig' %}

{% block title %}Inscription{% endblock %}

{% block body %}
	<div class="col-9">
		<h1>Sign up</h1>
		<hr>
		<form method="post">
			{% csrf_token %}
			{{ form.as_p }}
			<p class="centeredButton"><button type="submit" class="btn btn-info">Sign up</button></p>
		</form>
	</div>
{% endblock %}